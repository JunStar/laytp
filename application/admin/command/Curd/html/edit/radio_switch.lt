<!-- 隐藏域设置开关未选中时需要传递的参数值 -->
            <input type="hidden" name="row[{%field_name%}]" value="{%default_value%}" />
            <input type="checkbox" name="row[{%field_name%}]" lay-skin="switch" lay-text="{%lay_text%}" value="{%checked_value%}" {if {%checked_value%} == ${%field_name%}}checked="checked"{/if} />