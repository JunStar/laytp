<select name="search_param[{%field_name%}][value]" id="{%field_name%}"
                xm-select="{%field_name%}"
                xm-select-search="{%search_url%}"
                xm-select-search-field="{%search_field%}"
                xm-select-search-show-field="{%show_field%}"
                xm-select-search-condition="LIKE"
                xm-select-height="36px"
                {%max%}>
            </select>
            <input type="hidden" name="search_param[{%field_name%}][condition]" value="{%condition%}">