<select id="{%field%}" name="search_param[{%field%}][value]"
                xm-select="search_{%field%}"
                xm-select-search="{%search_url%}"
                xm-select-search-field="{%search_field%}"
                xm-select-search-show-field="{%show_field%}"
                xm-select-search-condition="LIKE"
                xm-select-height="36px"
                {%max%}>
            </select>
            <input type="hidden" name="search_param[{%field%}][condition]" value="{%condition%}">