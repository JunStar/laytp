<!-- 隐藏域设置开关未选中时需要传递的参数值 -->
            <input type="hidden" name="row[{%field_name%}]" id="{%field_name%}-{%un_checked_value%}" value="{%un_checked_value%}" />
            <input type="checkbox" name="row[{%field_name%}]" id="{%field_name%}-{%checked_value%}" lay-skin="switch" lay-text="{%lay_text%}" value="{%checked_value%}" {if {%checked_value%} == ${%field_name%}}checked="checked"{/if} />