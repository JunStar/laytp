    //删除
    public function del(){
        $ids = array_filter(explode(',', $this->request->param('ids')));
        if (!$ids) {
            return $this->error('参数ids不能为空');
        }
        if ($this->model->destroy($ids)) {
            return $this->success('数据删除成功');
        } else {
            return $this->error('数据删除失败');
        }
    }